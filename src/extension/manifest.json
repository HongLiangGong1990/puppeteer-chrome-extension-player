{
  "name": "puppeteer-chrome-extension-player",
  "description": "puppeteer-chrome-extension-player",
  "version": "0.0.0",
  "permissions": ["debugger"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["hoge.js"]
    }
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  },
  "manifest_version": 2
}
